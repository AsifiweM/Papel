{
  "name": "papel",
  "version": "1.0.0",
  "description": "A simple banking app",
  "main": "server/index.js",
  "scripts": {
    "create": "node ./server/database/db createTables",
    "dev": "nodemon ./dist/server/index.js",
    "start": "node ./server/index.js && npm run build && node dist/index.js",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AsifiweM/Papel.git"
  },
  "author": "Asifiwe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AsifiweM/Papel/issues"
  },
  "homepage": "https://github.com/AsifiweM/Papel#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "make-runnable": "^1.3.6",
    "pg": "^7.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}