{
  "name": "papel",
  "version": "1.0.0",
  "description": "A simple banking app",
  "main": "server/index.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
